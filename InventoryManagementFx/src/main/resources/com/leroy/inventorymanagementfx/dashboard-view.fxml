<BorderPane maxWidth="1.7976931348623157E308" prefHeight="510.0" prefWidth="428.0" style="-fx-background-color: #ffffff;" stylesheets="@style.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.leroy.inventorymanagementfx.controller.DashboardController">
    <center>
        <VBox maxWidth="1.7976931348623157E308" spacing="20" style="-fx-background-color: #f5f5f5;">
         <VBox style="-fx-background-color: white;">
            <children>
                  <HBox fx:id="dashboardContainer" minHeight="250" spacing="15" style="-fx-background-color: white;" />
            </children>
         </VBox>
         <VBox alignment="CENTER" maxWidth="1.7976931348623157E308" prefHeight="278.0" prefWidth="428.0" spacing="20.0" style="-fx-background-color: white;">
            <children>
               <Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="User Registration">
                  <font>
                     <Font name="Arial Bold" size="16.0" />
                  </font>
               </Label>

                        <VBox fx:id="formContainer" alignment="CENTER" maxWidth="500.0" minHeight="110.0" minWidth="350.0" prefHeight="110.0" prefWidth="428.0" style="-fx-background-color: white; -fx-padding: 15; -fx-border-width: 0;" />
